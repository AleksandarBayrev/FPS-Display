{
    "schemaVersion": 1,
    "id": "fpsdisplay",
    "version": "${version}",
    "name": "FPS-Display",
    "description": "This mod shows your current FPS on screen.",
    "authors": [
        "Grayray75"
    ],
    "contact": {
        "homepage": "https://github.com/Grayray75/FPS-Display",
        "sources": "https://github.com/Grayray75/FPS-Display",
        "issues": "https://github.com/Grayray75/FPS-Display/issues"
    },
    "license": "LGPL-3.0-only",
    "icon": "assets/fpsdisplay/icon.png",
    "environment": "client",
    "entrypoints": {
        "client": [
            "io.grayray75.mods.fpsdisplay.FpsDisplayMod"
        ],
        "modmenu": [
            "io.grayray75.mods.fpsdisplay.config.ModMenuIntegration"
        ]
    },
    "mixins": [
        "fpsdisplay.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.14.0",
        "minecraft": "1.20.x",
        "fabric-key-binding-api-v1": "*",
        "fabric-lifecycle-events-v1": "*"
    },
    "suggests": {
        "cloth-config": ">=11.0.0",
        "modmenu": ">=7.0.0"
    }
}
