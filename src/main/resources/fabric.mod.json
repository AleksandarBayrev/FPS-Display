{
    "schemaVersion": 1,
    "id": "fps-display",
    "version": "${version}",
    "name": "FPS-Display",
    "description": "A simple Fabric mod to show your ingame FPS",
    "authors": [
        "Grayray75"
    ],
    "contact": {
        "homepage": "https://github.com/Grayray75/FPS-Display",
        "sources": "https://github.com/Grayray75/FPS-Display"
    },
    "license": "CC0-1.0",
    "icon": "assets/modid/icon.png",
    "environment": "client",
    "entrypoints": {
        "client": [
            "io.grayray75.fabric.fpsdisplay.client.FpsDisplayClient"
        ]
    },
    "mixins": [
        "fps-display.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.9.0",
        "minecraft": "1.16.x"
    }
}